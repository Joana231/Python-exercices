chicken_menu = 10.35
fish_menu = 12.40
veg_menu = 8.15
delivery = 2.50

chicken_menu_count = int(input())
fish_menu_count = int(input())
veg_menu_count = int(input())

price = (chicken_menu_count * chicken_menu) + (fish_menu_count * fish_menu) + (veg_menu_count*veg_menu)
sweet_price = price * (20/100)

final_price = (price + sweet_price) + delivery
print(final_price)