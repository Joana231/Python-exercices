number_1 = int(input())
number_2 = int(input())
op = str(input())

if op == "+":
    result = number_1 + number_2
    if result % 2 == 0:
        print(f"{number_1} {op} {number_2} = {result} - even")
    else:
        print(f"{number_1} {op} {number_2} = {result} - odd")


elif op == "-":
    result = number_1 - number_2
    if result % 2 == 0:
        print(f"{number_1} {op} {number_2} = {result} - even")
    else:
        print(f"{number_1} {op} {number_2} = {result} - odd")

elif op == "*":
    result = number_1 * number_2
    if result % 2 == 0:
        print(f"{number_1} {op} {number_2} = {result} - even")
    else:
        print(f"{number_1} {op} {number_2} = {result} - odd")

elif op == "/":
    if number_2 == 0:
        print(f"Cannot divide {number_1} by zero")
    else:
        result = number_1 / number_2
        print(f"{number_1} {op} {number_2} = {result:.2f}")

elif op == "%":
    if number_2 == 0:
        print(f"Cannot divide {number_1} by zero")
    else:
        result = number_1 % number_2
        print(f"{number_1} {op} {number_2} = {result}")